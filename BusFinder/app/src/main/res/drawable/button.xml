        android:angle="45"
        android:centerX="25%"
        android:centerColor="#B2BDA4"
        android:startColor="#F5F4ED"
        android:endColor="#5F3B1E"
        android:type="linear"
        />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"
        />
